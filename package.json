    "react": "16.9.0",
    "react-animations": "^1.0.0",
    "react-datetime": "2.16.3",
    "react-dom": "16.9.0",
    "react-particles-js": "^2.7.0",
    "react-pose": "^4.0.10",
    "react-pose-text": "^3.1.0",
    "react-preloaders": "^3.0.3",
    "react-reveal": "^1.2.2",
    "react-router-dom": "5.0.1",
    "react-scripts": "3.1.0",
